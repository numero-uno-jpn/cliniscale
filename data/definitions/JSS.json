        {
            "dispname": "報酬満足度スコア（項目1,10,19,28）",
            "name": "報酬満足度",
            "type": "eval",
            "formula": "[[Q1.index]] + (7 - [[Q10.index]]) + (7 - [[Q19.index]]) + [[Q28.index]]"
        },
        {
            "dispname": "昇進満足度スコア（項目2,11,20,33）",
            "name": "昇進満足度",
            "type": "eval",
            "formula": "(7 - [[Q2.index]]) + [[Q11.index]] + [[Q20.index]] + [[Q33.index]]"
        },
        {
            "dispname": "監督満足度スコア（項目3,12,21,30）",
            "name": "監督満足度",
            "type": "eval",
            "formula": "[[Q3.index]] + (7 - [[Q12.index]]) + (7 - [[Q21.index]]) + [[Q30.index]]"
        },
        {
            "dispname": "福利厚生満足度スコア（項目4,13,22,29）",
            "name": "福利厚生満足度",
            "type": "eval",
            "formula": "(7 - [[Q4.index]]) + [[Q13.index]] + [[Q22.index]] + (7 - [[Q29.index]])"
        },
        {
            "dispname": "成果報酬満足度スコア（項目5,14,23,32）",
            "name": "成果報酬満足度",
            "type": "eval",
            "formula": "[[Q5.index]] + (7 - [[Q14.index]]) + (7 - [[Q23.index]]) + (7 - [[Q32.index]])"
        },
        {
            "dispname": "手続き満足度スコア（項目6,15,24,31）",
            "name": "手続き満足度",
            "type": "eval",
            "formula": "(7 - [[Q6.index]]) + [[Q15.index]] + (7 - [[Q24.index]]) + (7 - [[Q31.index]])"
        },
        {
            "dispname": "同僚満足度スコア（項目7,16,25,34）",
            "name": "同僚満足度",
            "type": "eval",
            "formula": "[[Q7.index]] + (7 - [[Q16.index]]) + [[Q25.index]] + (7 - [[Q34.index]])"
        },
        {
            "dispname": "仕事の性質満足度スコア（項目8,17,27,35）",
            "name": "仕事の性質満足度",
            "type": "eval",
            "formula": "(7 - [[Q8.index]]) + [[Q17.index]] + [[Q27.index]] + [[Q35.index]]"
        },
        {
            "dispname": "コミュニケーション満足度スコア（項目9,18,26,36）",
            "name": "コミュニケーション満足度",
            "type": "eval",
            "formula": "[[Q9.index]] + (7 - [[Q18.index]]) + (7 - [[Q26.index]]) + (7 - [[Q36.index]])"
        },
        {
            "dispname": "総合満足度スコア（全36項目の合計）",
            "name": "総合満足度",
            "type": "eval",
            "formula": "[[報酬満足度]] + [[昇進満足度]] + [[監督満足度]] + [[福利厚生満足度]] + [[成果報酬満足度]] + [[手続き満足度]] + [[同僚満足度]] + [[仕事の性質満足度]] + [[コミュニケーション満足度]]"
        },
        {
            "dispname": "満足度レベル判定",
            "name": "満足度レベル",
            "type": "eval",
            "formula": "[[総合満足度]] >= 144 ? '満足' : ( [[総合満足度]] >= 108 ? '中立' : '不満足' )"
        }
    ]
