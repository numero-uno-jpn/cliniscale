            "formula": "{{診断目的}} + ': ' + {{主訴}}"
        },
        {
            "name": "精神症重症度",
            "type": "eval",
            "dispname": "精神症の重症度評価",
            "formula": "{{幻覚有無}} == 'あり' || {{妄想有無}} == 'あり' || {{思考障害有無}} == 'あり' ? ([[機能障害.index]] >= 2 ? '重度' : '中等度') : '軽度'"
        },
        {
            "name": "気分症重症度",
            "type": "eval",
            "dispname": "気分症の重症度評価",
            "formula": "{{抑うつ症状有無}} == 'あり' || {{躁症状有無}} == 'あり' ? ([[機能障害.index]] >= 2 ? '重度' : '中等度') : '軽度'"
        },
        {
            "name": "不安症重症度",
            "type": "eval",
            "dispname": "不安症の重症度評価",
            "formula": "([[全般性不安.index]] > 0 ? 1 : 0) + ({{パニック症状有無}} == 'あり' ? 1 : 0) + ({{恐怖症有無}} == 'あり' ? 1 : 0) >= 1 ? ([[機能障害.index]] >= 2 ? '重度' : '中等度') : '軽度'"
        },
        {
            "name": "神経発達症重症度",
            "type": "eval",
            "dispname": "神経発達症の重症度評価",
            "formula": "([[知的発達.index]] > 0 ? 1 : 0) + ({{ASD症状有無}} == 'あり' ? 1 : 0) + ({{ADHD症状有無}} == 'あり' ? 1 : 0) >= 1 ? ([[機能障害.index]] >= 2 ? '重度' : '中等度') : '軽度'"
        },
        {
            "name": "治療緊急度",
            "type": "eval",
            "dispname": "治療の緊急度評価",
            "formula": "{{抑うつ症状}}.includes('希死念慮') || {{抑うつ症状}}.includes('自殺企図') || [[機能障害.index]] >= 3 ? '緊急' : ([[機能障害.index]] >= 2 ? '高' : '中')"
        },
        {
            "name": "Emergency: 治療緊急度が緊急",
            "formula": "{{治療緊急度}} == '緊急'",
            "type": "emergency"
        },
        {
            "name": "Warning: 機能障害が中等度以上",
            "formula": "[[機能障害.index]] >= 2",
            "type": "warning"
        }
    ]
