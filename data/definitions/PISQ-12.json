    {
      "type": "eval",
      "name": "行動感情スコア",
      "formula": "[[PISQ1.index]] + [[PISQ2.index]] + [[PISQ3.index]] + [[PISQ4.index]]",
      "noreport": true
    },
    {
      "type": "eval",
      "name": "身体スコア",
      "formula": "[[PISQ5.index]] + [[PISQ6.index]] + [[PISQ7.index]] + [[PISQ8.index]] + [[PISQ9.index]]",
      "noreport": true
    },
    {
      "type": "eval",
      "name": "パートナースコア",
      "formula": "[[PISQ10.index]] + [[PISQ11.index]] + [[PISQ12.index]]",
      "noreport": true
    },
    {
      "type": "eval",
      "name": "PISQ12合計スコア",
      "formula": "[[行動感情スコア]] + [[身体スコア]] + [[パートナースコア]]"
    },
    {
      "type": "eval",
      "name": "性機能評価",
      "formula": "[[PISQ12合計スコア]] >= 35 ? '良好' : ( [[PISQ12合計スコア]] >= 25 ? '中程度' : '低下' )"
    },
    {
      "type": "warning",
      "name": "Warning: 中程度リスク PISQ-12スコア < 35",
      "formula": "[[PISQ12合計スコア]] < 35"
    },
    {
      "type": "emergency",
      "name": "Emergency: 高リスク PISQ-12スコア < 25",
      "formula": "[[PISQ12合計スコア]] < 25"
    }
